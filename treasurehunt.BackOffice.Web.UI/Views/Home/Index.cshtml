@model IEnumerable<treasurehunt.Core.Data.Models.Quetes.Evenement>

@{
    ViewData["Title"] = "Home Page";
}


<h1>Les Evenement</h1>

@foreach (var evenement in Model)
{
    <div class="row">
        <div class="col s12 m6">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">@evenement.Titre</span>
                    <p>
                        @evenement.Description
                    </p>
                </div>
                <div class="card-action">
                    <a asp-area="" asp-controller="Quete" asp-action="Edit" asp-route-id="@evenement.Id">Modifier</a>
                </div>
            </div>
        </div>
    </div>
}

